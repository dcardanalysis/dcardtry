<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.dcardtry">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission
        android:name="android.permission.WRITE_SETTINGS"
        tools:ignore="ProtectedPermissions" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dcardtry">
        <activity
            android:name=".ManagerChangePassword"
            android:exported="true" />
        <activity
            android:name=".Manager_AddAccount"
            android:exported="true" />
        <activity
            android:name=".UserChangePassword"
            android:exported="true" />
        <activity
            android:name=".ManagerPage"
            android:exported="true" />
        <activity
            android:name=".ArticlePageActivity"
            android:exported="true" />
        <activity android:name=".HomePage" />
        <activity android:name=".Manager_entry" />
        <activity android:name=".CreateAccount" />
        <activity
            android:name=".DcardDetailActivity"
            android:exported="false" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>